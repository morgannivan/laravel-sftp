{
    "name": "neoxia/laravel-sftp",
    "license": "MIT",
    "description": "SFTP filesystem service provider for Laravel",
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/filesystem": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "league/flysystem-sftp": "1.0.*"
    },
    "autoload": {
         "psr-4": {
            "Neoxia\\": "src/Neoxia/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Neoxia\\Filesystem\\SftpServiceProvider"
            ]
        }
    }
}
